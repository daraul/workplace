<%= form_for(@job, html: {multipart: true}) do |f| %>
    <ul>
        <% @job.errors.full_messages.each do |error| %>
            <li><%= error %></li>
        <% end %>
    </ul>
    <p>
        <%= f.label :title %><br />
        <%= f.text_field :title %>
    </p>
    <p>
        <%= f.label :description %><br />
        <%= f.text_area :description %>
    </p>
    <p>
        <%= f.label :employee_id %>
        <!--<%= f.select :employee_id, [["john", 1], ["jacob", 2]] %>-->
        <%= f.collection_select(:employee_id, Employee.all, :id, :first_name) %>
    </p>
    <p>
        <%= f.label :complete %>
        <%= f.check_box :complete %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>